{
  "name": "@smplrspace/smplr-loader",
  "version": "2.8.4",
  "description": "NPM package to load a typed Smplr.js from CDN easily",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn tsc && vite build",
    "dev": "echo 'No dev'",
    "serve": "echo 'No serve'",
    "libtest": "echo 'No libtest'",
    "tsc": "tsc --noEmit",
    "lint": "eslint . && prettier --check **/*.json && yarn tsc",
    "lint:fix": "eslint --fix . && prettier -w **/*.json"
  },
  "lint-staged": {
    "*.ts": [
      "yarn run eslint"
    ],
    "*.json": [
      "yarn run prettier --check"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "4.7.4",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.1"
  },
  "homepage": "https://www.smplrspace.com",
  "keywords": [
    "smplrspace",
    "floor plan",
    "digital twin"
  ],
  "packageManager": "yarn@3.2.4"
}
